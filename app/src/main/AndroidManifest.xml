<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.itborneo.laundrymanage">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LaundryManage"
        android:usesCleartextTraffic="true">
        <activity android:name=".transaction.TransactionAddUpdateActivity"></activity>
        <activity android:name=".packet.PacketAddUpdateActivity" />
        <activity android:name=".HomeActivity"></activity>
        <activity android:name=".ReportActivity" />
        <activity android:name=".transaction.TransactionActivity" />
        <activity android:name=".users.UserManageActivity" />
        <activity android:name=".packet.PacketActivity" />

        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>